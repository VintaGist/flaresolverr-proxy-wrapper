# Port for the wrapper service
WRAPPER_PORT=8191

# Internal FlareSolverr URL
FLARESOLVERR_URL=http://flaresolverr:8191

# Starting port for temporary proxies
LOCAL_PROXY_PORT_START=4141

# Maximum concurrent proxy sessions
MAX_CONCURRENT_PROXIES=20

# Proxy timeout in milliseconds
PROXY_TIMEOUT=120000

# Environment mode
NODE_ENV=production